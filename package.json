{
  "name": "NOTS-Hair-Admin-Dashboard",
  "version": "1.0.0",
  "description": "An admin's GUI for NOTS, designed for tracking orders and visualizing the customer data base.",
  "main": "functions/index.js",
  "scripts": {
    "start": "node functions/index.js",
    "start-dev": "cross-env NODE_ENV=development nodemon functions/index.js",
    "test-jest": "jest ./__tests__",
    "cypress:open": "cypress open --config baseUrl=http://localhost:8080",
    "cypress:run": "cypress run --browser chrome --headed",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --mode production",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon functions/index.js\" \"cross-env NODE_ENV=development webpack-dev-server --hot --open --history-api-fallback\"",
    "docker-build": "docker build --tag nots .",
    "docker-run": "docker run --publish 3000:3000 nots"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshick97/NOTS-Admin-Dashboard.git"
  },
  "keywords": [
    "e-commerce",
    "dashboard",
    "administrative panel"
  ],
  "author": "Maxwell Shick",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mshick97/NOTS-Admin-Dashboard/issues"
  },
  "homepage": "https://github.com/mshick97/NOTS-Admin-Dashboard#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-jest": "^29.2.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "firebase-functions-test": "^0.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.9.2",
    "webpack-config": "^7.0.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@mui/material": "^5.8.3",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.16.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cypress": "^11.2.0",
    "dotenv": "^16.0.2",
    "escape-string-regexp": "^5.0.0",
    "express": "^4.18.1",
    "firebase": "^9.9.4",
    "firebase-admin": "^10.3.0",
    "firebase-functions": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "pg": "^8.8.0",
