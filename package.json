{
  "name": "NOTS-Hair-Admin-Dashboard",
  "version": "1.0.0",
  "description": "An admin's GUI for NOTS, designed for tracking orders and visualizing the customer data base.",
  "main": "functions/index.js",
  "scripts": {
    "start": "node functions/index.js",
    "production-start": "cross-env NODE_ENV=production node functions/index.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon functions/index.js\" \"cross-env NODE_ENV=development webpack-dev-server --hot --open\"",
    "docker-build": "docker build --tag nots .",
    "docker-run": "docker run --publish 3000:3000 nots"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshick97/NOTS-Admin-Dashboard.git"
  },
  "keywords": [
    "e-commerce",
    "dasboard",
    "administrative panel"
  ],
  "author": "Maxwell Shick",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mshick97/NOTS-Admin-Dashboard/issues"
  },
  "homepage": "https://github.com/mshick97/NOTS-Admin-Dashboard#readme",
